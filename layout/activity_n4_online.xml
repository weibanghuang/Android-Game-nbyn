        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
        android:background="@drawable/no_player"
        app:layout_constraintBottom_toTopOf="@id/line2h"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toStartOf="@id/line1v"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/line1h" />

    <ImageView
        android:id="@+id/button11"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
        android:background="@drawable/no_player"
        app:layout_constraintBottom_toTopOf="@id/line2h"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toStartOf="@id/line2v"
        app:layout_constraintStart_toStartOf="@id/line1v"
        app:layout_constraintTop_toTopOf="@id/line1h" />

    <ImageView
        android:id="@+id/button12"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
        android:background="@drawable/no_player"
        app:layout_constraintBottom_toTopOf="@id/line2h"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toStartOf="@id/line3v"
        app:layout_constraintStart_toStartOf="@id/line2v"
        app:layout_constraintTop_toTopOf="@id/line1h" />

    <ImageView
